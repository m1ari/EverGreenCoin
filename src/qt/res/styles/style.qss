}

QVBoxLayout {
        border-radius: 6px;
        border-width: 0;
        border-style: none;
        padding: 8px;
        background-color: #153f30;
        color: 1ab06c;
}

QDialog {
        color: #ffffff;
        background-color: #153f30;
}

QMainWindow {
        color: #ffffff;
        background-color: #153f30;
        border: 2px solid #018457;
}

QMenu {
        background-color: #153f30;
        color: #ffffff;
}

QLabel {
        background-color: transparent;
        color: #ffffff;
}

QMessageBox {
        color: #ffffff;
}

QCheckBox {
        border-radius: 2px;
        border-width: 2;
        border-style: none;
        padding: 3px; 
        background-color: transparent;
        color: #ffffff;
}

