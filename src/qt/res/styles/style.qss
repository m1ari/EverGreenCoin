        border-radius: 6px;
        text-align: center;
        color: #ffffff;
}

QProgressBar::chunk {
        background: #018457;
        border-radius: 6px; margin: 0px;
}

QScrollArea {
        border-radius: 6px;
        border-width: 1;
        border-style: solid;
        border-color: #018457;
        padding: 8px;
        color: #ffffff;
}

QScrollArea #scrollAreaWidgetContents {
