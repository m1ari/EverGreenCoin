QRadioButton {
        background: transparent;
        color: #ffffff;
}

QFrame#central {
        background-color: #153f30;
}

QFrame#frameBlocks {
        border: none;
        background: #153f30;
}

QFrame#frame, QFrame#frame_2, QFrame#frame_3, QFrame#gridFrame_2, QFrame#SendCoinsEntry, QFrame#frameCoinControl {
        background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #155f30, stop: 0.05 #018457, stop: 0.95 #018457, stop: 1 #155f30);
        border-style: plain;
        border-width: 20px;
        border-radius: 15px;
        border-color: #BCED91;
        min-width: 10em;
        padding-left: 15px;
        padding-right: 15px;
        color: #ffffff;
}

QPushButton, QToolButton {
        border-radius: 6px;
        background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #018457, stop: 0.18 #1ab06c, stop: 0.85 #1ab06c, stop: 1 #018457);
        color: #ffffff;
        padding: 7px;
        margin: 5px;
}

QPushButton:hover, QToolButton:hover {
        color: #ffffff;
}

QPushButton:pressed, QToolButton:checked {
        color: #ffffff;
        background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #1ab06c, stop: 0.18 #018457, stop: 0.85 #018457, stop: 1 #1ab06c);
}

QPushButton:pressed, QToolButton:pressed {
        color: #1ab06c;
        background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #018457, stop: 0.25 #0f5127, stop: 0.75 #0f5127, stop: 1 #018457);
}

QPushButton:disabled, QToolButton:disabled {
        color: #018457;
        background: #35473c;
}

/* the background-color is overridden to transparent for mac os x
   due to the entire toolbar area not being drawable in a regular
   QMainWindow
*/
QToolBar {
        background: #153f30;
        border: 1px solid #018457;
        padding: 3px;
}

QToolBar::handle {
        image: url(handle.png);
}

QStatusBar {
        background: #153f30;
}

QMenuBar {
        background: #153f30;
        }

QMenuBar::item {
        background: #153f30;
        color: #ffffff;
        }

QMenuBar::hovered {
        background: #018457;
        }

QLineEdit {
        border: 2px solid #153f30;
        border-radius: 3px;
        background-color: #018457;
        color: white;
        padding: 7px;
}

QTextEdit {
        border: 3px solid #1ab06c;
        border-radius: 6px;
        background: #153f30;
        color: #ffffff;
        padding: 7px;
}

QSpinBox {
        border: 2px solid #153f30;
        border-radius: 4px;
        padding: 2px;
        background-color: #018457;
        color: #ffffff;
}

QDoubleSpinBox {
        border: 2px solid #ffffff;
        border-radius: 4px;
        padding: 2px;
        background-color: #018457;
        color: #ffffff;
}

QComboBox {
        border: 2px solid #ffffff;
        border-radius: 3px;
        color: #ffffff;
        background-color: #018457;
        min-width: 4em;
}

QComboBox::drop-down {
        border-width: 2px;
        border-style: 2px solid #ffffff;
        background-color: #018457;
        color: #ffffff;
}

QComboBox QAbstractItemView {
        background-color: #153f30;
        color: #ffffff;
}

QTableView {
        background: #153f30;
        color: #ffffff;
        border-style: none;
        selection-background-color: #1ab06c;
        alternate-background-color: #018457;
        selection-color: #FFFFFF;
        border: 2px solid #018457;
}

QHeaderView { color: #ffffff; background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #155f30, stop: 0.05 #018457, stop: 0.95 #018457, stop: 1 #155f30); background-image: none; qproperty-defaultAlignment: AlignHCenter; }
QHeaderView::section { color: #ffffff; background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #155f30, stop: 0.05 #018457, stop: 0.95 #018457, stop: 1 #155f30); background-image: none; qproperty-defaultAlignment: AlignHCenter; }
QHeaderView::section:checked { background-color: #018457; }

QTabWidget {
        border: 2px solid #018457;
        border-radius: 6px;
        background-color: #153f30;
        color: #ffffff;
        padding: 7px;
}

QTabWidget::pane {
        border-top: 4px solid #018457;
        border: 3px solid #018457;
        border-radius: 6px;
        background: QLinearGradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #155f30, stop: 0.05 #018457, stop: 0.95 #018457, stop: 1 #155f30);
        padding-left: 15px;
        padding-right: 15px;
}

QTabWidget::tab-bar {
        left: 30px; /* move to the right */
}

 /* Style the tab using the tab sub-control. Note that
     it reads QTabBar _not_ QTabWidget */

QTabBar::tab {
        background: #153f30;
        border: 2px solid #018457;
        border-bottom-color: #153f30; /* same as the pane color */
        border-top-left-radius: 4px;
        border-top-right-radius: 4px;
        min-width: 8ex;
        padding: 4px;
        color: #1ab06c;
}

QTabBar::tab:selected { /*, QTabBar::tab:hover { */
        color: #FFFFFF;
        background: #018457;
        border: 2px solid #018457;
        border-bottom-color: #018457;
}

QTabBar::tab:selected {
        border-color: #018457; /* same as pane color */
        border-top-color: #018457;
}

QTabBar::tab:!selected {
        margin-top: 2px; /* make non-selected tabs look smaller */
}

 /* make use of negative margins for overlapping tabs */
QTabBar::tab:selected {
     /* expand/overlap to the left and right by 4px */
        margin-left: -4px;
        margin-right: -4px;
}

QTabBar::tab:first:selected {
        margin-left: 0; /* the first selected tab has nothing to overlap with on the left */
}

QTabBar::tab:last:selected {
        margin-right: 0; /* the last selected tab has nothing to overlap with on the right */
}

QTabBar::tab:only-one {
        margin: 0; /* if there is only one tab, we don't want overlapping margins */
}
